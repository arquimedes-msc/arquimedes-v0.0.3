# Modo de banco de dados: use "supabase" para PostgreSQL ou "manus" para MySQL/TiDB
DB_MODE=supabase

# Configurações do Supabase (obrigatórias quando DB_MODE=supabase)
SUPABASE_URL=https://<sua-instancia>.supabase.co
SUPABASE_ANON_KEY=<chave-publica>
SUPABASE_SERVICE_KEY=<chave-de-servico>
SUPABASE_DATABASE_URL=postgresql://postgres:<senha>@<host>:5432/postgres

# Configuração alternativa para Manus (MySQL/TiDB)
DATABASE_URL=mysql://user:password@host:3306/database
